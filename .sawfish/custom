(custom-set-keymap (quote global-keymap) (quote (keymap (cycle-windows . "M-TAB") (destroy-window . "M-F4") (send-to-workspace-left . "M-S-Left") (send-to-workspace-right . "M-S-Right") ((run-shell-command "urxvt -bg white +sb -bd white -e screen -xRR") . "M-`") ((run-shell-command "urxvt -bg white +sb -bd white") . "M-~") (raise-window-depth . "M-Home") (lower-window-depth . "M-End") ((run-shell-command "emacs") . "XF86Mail") (maximize-window-vertically-toggle . "H-Prior") (maximize-window-horizontally-toggle . "H-Next") ((activate-workspace 1) . "C-1") ((activate-workspace 2) . "C-2") ((activate-workspace 3) . "C-3") ((activate-workspace 4) . "C-4") ((activate-workspace 5) . "C-5") ((activate-workspace 6) . "C-6") (maximize-window-toggle . "M-F10") (next-workspace . "W-Right") (previous-workspace . "W-Left"))))
(custom-set-typed-variable (quote default-frame-style) (quote Studio) (quote frame-style))
(custom-set-typed-variable (quote match-window-profile) (quote ((((WM_CLASS . "^Epiphany/epiphany$") (WM_CLASS . "irefox") (WM_CLASS . "Xulrunner")) (workspace . 1) (maximized . all) (frame-type . none)) (((WM_NAME . "deskbar-applet")) (frame-type . none)) (((WM_NAME . "^urxvt$")) (position 20 . 710) (maximized . vertical) (frame-type . none)) (((WM_CLASS . "^Emacs/emacs$")) (position 20 . 0) (maximized . vertical) (frame-type . none)))) (quote match-window) (quote sawfish.wm.ext.match-window))
(custom-set-typed-variable (quote move-show-position) (quote ()) (quote boolean))
(custom-set-typed-variable (quote default-font) (quote ("Pango" . "Terminus 8")) (quote font))
(custom-set-keymap (quote window-keymap) (quote (keymap (raise-and-pass-through-click . "Button1-Click") (resize-window-interactively . "M-Button3-Click") (move-window-interactively . "W-Button1-Move") (popup-window-menu . "W-Button2-Click") (raise-lower-window . "W-Button3-Click") (lower-window . "W-Down") (raise-window . "W-Up"))))
